package br.unifor.Panels;

import java.awt.Color;

import javax.swing.JPanel;

public class PainelEsquerdo extends JPanel implements IPainel {

	private PainelEsquerdo(){
		config();
	}
	
	
	
	private static PainelEsquerdo instance = null;
	public static PainelEsquerdo getInstance(){
		return instance == null ? instance = new PainelEsquerdo() : instance;
	}
	@Override
	public void config() {
		this.setBackground(Color.BLACK);		
	}

}
